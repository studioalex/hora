{
  "name": "@studioalex/hora-grid",
  "private": true,
  "license": "GPLV3+",
  "version": "0.1.5",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/hora-grid.umd.cjs",
  "module": "./dist/hora-grid.js",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/hora-grid.js",
      "require": "./dist/hora-grid.udm.cjs"
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/style.css"
  },
  "unpkg": "./dist/hora-grid.umd.cjs",
  "jsdelivr": "./dist/hora-grid.umd.cjs",
  "scripts": {
    "dev": "vue-tsc --noEmit && vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "postbuild": "cpy src/types/*.d.ts dist/types --flat",
    "lint": "eslint --ext .js,.ts,.vue src",
    "clean": "del-cli ./node_modules ./dist ./src/dist ./coverage",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:why": "tsc --noEmit --explainFiles > explainTypes.txt"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@studioalex/hora-elements": "*",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/ui": "^0.23.1",
    "@vue/eslint-config-typescript": "^11.0.1",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "cpy-cli": "^4.2.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "happy-dom": "^6.0.4",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vitest": "^0.23.1",
    "vue-draggable-next": "^2.1.1",
    "vue-tsc": "^0.40.13"
  }
}
