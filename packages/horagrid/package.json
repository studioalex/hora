{
  "name": "@studioalex/horagrid",
  "private": true,
  "license": "GPLV3+",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/horagrid.umd.cjs",
  "module": "./dist/horagrid.js",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/horagrid.js",
      "require": "./dist/horagrid.udm.cjs"
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/style.css"
  },
  "unpkg": "./dist/horagrid.umd.cjs",
  "jsdelivr": "./dist/horagrid.umd.cjs",
  "scripts": {
    "dev": "vue-tsc --noEmit && vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "postbuild": "cpy src/types/*.d.ts dist/types --flat",
    "lint": "eslint --ext .js,.ts,.vue src",
    "clean": "del-cli ./node_modules ./dist ./src/dist ./coverage",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitest/ui": "^0.21.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "cpy-cli": "^4.1.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^6.0.4",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "vitest": "^0.21.1",
    "vue-draggable-next": "^2.1.1",
    "vue-tsc": "^0.39.5"
  }
}
