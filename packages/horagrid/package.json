{
  "name": "@studioalex/horagrid",
  "private": true,
  "license": "GPLV3+",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/horagrid.umd.js",
  "module": "./dist/horagrid.es.js",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/horagrid.es.js",
      "require": "./dist/horagrid.udm.js"
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/style.css"
  },
  "unpkg": "./dist/horagrid.umd.js",
  "jsdelivr": "./dist/horagrid.umd.js",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "postbuild": "cpy src/types/*.d.ts dist/types --flat",
    "lint": "eslint --ext .js,.ts,.vue src",
    "clean": "del-cli ./node_modules ./dist",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "cpy-cli": "^4.1.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.18.0",
    "eslint-plugin-vue": "^9.1.1",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.17.1",
    "vue-tsc": "^0.38.2"
  }
}
