{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["lint", "^build"],
      "outputs": ["dist/**", "docs/.vitepress/dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["@studioalex/horagrid#build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["lint"]
    },
    "clean": {
      "outputs": []
    },
    "deploy:clean": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["deploy:clean", "test", "build"],
      "outputs": ["deploy/**"]
    }
  }
}
