{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["@studioalex/horagrid#build"],
      "cache": false
    },
    "clean": {
      "outputs": []
    },
    "deploy:clean": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["deploy:clean", "build"],
      "outputs": []
    }
  }
}
