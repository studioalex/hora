{
  "name": "@studioalex/hora-docs",
  "version": "0.1.10",
  "description": "Hora Grid Vue 3 Component Documentation",
  "main": "index.js",
  "scripts": {
    "devs": "vitepress dev docs --host",
    "dev": "vitepress dev docs --host --port 3333 --open",
    "build": "vitepress build docs",
    "serve": "vitepress --port 3330 serve docs",
    "deploy": "cpy . ../../../../../deploy/web --cwd=docs/.vitepress/dist",
    "clean": "del-cli ./node_modules ./docs/.vitepress/dist"
  },
  "keywords": [
    "API",
    "documentation",
    "docu",
    "vitepress",
    "vue",
    "press"
  ],
  "author": "Alexander Sedeke <alexander.sedeke@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@studioalex/hora-elements": "*",
    "@studioalex/hora-grid": "*",
    "cpy-cli": "^4.2.0",
    "del-cli": "^5.0.0",
    "lodash": "^4.17.21",
    "vitepress": "^1.0.0-alpha.13",
    "vue": "^3.2.39"
  }
}
